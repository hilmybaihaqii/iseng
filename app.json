{
  "expo": {
    "name": "collab",
    "slug": "collab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "collab",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true, // Note: newArchEnabled might have compatibility issues with some libraries. Ensure all your dependencies support it.
    "ios": {
      "supportsTablet": true
      // You might need to add "infoPlist" for permissions if not using plugins
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-video",

      [
        "expo-image-picker",
        {
          "photosPermission": "Izinkan $(PRODUCT_NAME) mengakses galeri foto Anda untuk memilih gambar profil.",
          "cameraPermission": "Izinkan $(PRODUCT_NAME) mengakses kamera Anda untuk mengambil gambar profil."
        }
      ]

    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}